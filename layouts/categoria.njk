{% extends "base.njk" %}
{% block content %}
<h1>{{ title }}</h1>
<p>{{ descricao_categoria }}</p>
<ul>
  {% for tecnologia in collections.tecnologias %}
    {% if tecnologia.data.categoria == categoria %}
      <li><a href="{{ tecnologia.url }}">{{ tecnologia.data.title }}</a></li>
    {% endif %}
  {% endfor %}
</ul>
{% endblock %}
